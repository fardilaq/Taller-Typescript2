{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "rootDir": "./Taller",
    "outDir": "./scripts",
    "strict": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "isolatedModules": true,
    "declaration": false,
    "sourceMap": false
  },
  "include": ["Taller/**/*.ts"],
  "exclude": ["scripts","node_modules"]
}
